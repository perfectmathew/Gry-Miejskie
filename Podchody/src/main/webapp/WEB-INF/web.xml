<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>TaskServlet</servlet-name>
        <servlet-class>com.mycompany.podchody.TaskServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExamsServlet</servlet-name>
        <servlet-class>com.mycompany.podchody.EmxamsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GamesServlet</servlet-name>
        <servlet-class>com.mycompany.podchody.GamesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GameDetail</servlet-name>
        <servlet-class>com.mycompany.podchody.GameDetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddGame</servlet-name>
        <servlet-class>com.mycompany.podchody.AddGame</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GamesFunction</servlet-name>
        <servlet-class>com.mycompany.podchody.GamesFunction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>com.mycompany.podchody.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExamFunction</servlet-name>
        <servlet-class>com.mycompany.podchody.ExamFunction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TaskServlet</servlet-name>
        <url-pattern>/e</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExamsServlet</servlet-name>
        <url-pattern>/exams</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GamesServlet</servlet-name>
        <url-pattern>/games</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GameDetail</servlet-name>
        <url-pattern>/g</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddGame</servlet-name>
        <url-pattern>/games&amp;action=add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GamesFunction</servlet-name>
        <url-pattern>/function</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>home</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>ExamFunction</servlet-name>
        <url-pattern>/efunction</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
